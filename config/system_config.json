{
  "system": {
    "name": "AiKre8tive Sovereign Agent System",
    "version": "1.0.0",
    "environment": "development"
  },
  "agents": {
    "planetary_agents": [
      "Sun", "Mercury", "Venus", "Earth", "Mars", 
      "Jupiter", "Saturn", "Uranus", "Neptune", "Pluto"
    ],
    "moon_agents": [
      "Luna", "Phobos", "Deimos", "Io", "Europa", "Ganymede", 
      "Callisto", "Titan", "Enceladus", "Triton", "Charon"
    ],
    "dwarf_planet_agents": [
      "Ceres", "Eris", "Haumea", "Makemake"
    ]
  },
  "communication": {
    "protocol": "whisper_sync",
    "sync_interval": 30,
    "max_retries": 3,
    "timeout": 10
  },
  "logging": {
    "level": "INFO",
    "max_file_size": "10MB",
    "backup_count": 5
  }
}
